{
  "name": "haidar.io",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel src/index.html ",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url ./",
    "deploy": "rimraf dist && yarn build && cp CNAME dist/CNAME && gh-pages -d dist"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-social-icons": "^2.8.1"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.1",
    "gh-pages": "^1.2.0",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.9.1",
    "rimraf": "^2.6.2"
  }
}
